			
<% @courts.each do |court| %>
<% if court.loco_evaluation && court.loco_evaluatee == current_user %>
					

		<button class="btn btn-default btn-xs" type="button" data-toggle="collapse" data-target="#collapseLocoEval_<%= court.loco_evaluation.id %>" aria-expanded="false" aria-controls="collapseExample">
			<% if court.loco_evaluation.evaluation == "happy" %>
	           <i class="fa fa-smile-o fa-lg fa-success"></i>
	      <% elsif court.loco_evaluation.evaluation == "ok" %>
	            <i class="fa fa-meh-o fa-lg fa-warning"></i>
	      <% else %>
	            <i class="fa fa-frown-o fa-lg fa-danger"></i>
	      <% end %>
			
		</button>

              
		
				
	<% end %>
<% end %>


<% @courts.each do |court| %>
<% if court.dominus_litis_evaluation && court.dominus_litis_evaluatee == current_user %>
		
		<button class="btn btn-default btn-xs" type="button" data-toggle="collapse" data-target="#collapseDLEval_<%= court.dominus_litis_evaluation.id %>" aria-expanded="false" aria-controls="collapseExample">
			<% if court.dominus_litis_evaluation.evaluation == "happy" %>
	           <i class="fa fa-smile-o fa-lg fa-success"></i>
	      <% elsif court.dominus_litis_evaluation.evaluation == "ok" %>
	            <i class="fa fa-meh-o fa-lg fa-warning"></i>
	      <% else %>
	            <i class="fa fa-frown-o fa-lg fa-danger"></i>
	      <% end %>
		</button>

		
					
	<% end %>

<% end %>
<br>
<br>
<% @courts.each do |court| %>
<% if court.loco_evaluation && court.loco_evaluatee == current_user %>
					
		<div class="collapse" id="collapseLocoEval_<%= court.loco_evaluation.id %>">
			
			<ul class="list-group">
		        <li class="list-group-item">
			        
			        "<i><%= court.loco_evaluation.comment %></i>"
		             - Me <%= link_to court.user.last_name_or_placeholder, court_path(court) %>
			    </li>
			</ul>
		</div>
				
	<% end %>
<% end %>

<% @courts.each do |court| %>
<% if court.dominus_litis_evaluation && court.dominus_litis_evaluatee == current_user %>
		
		

