<p id="notice"><%= notice %></p>

<h1>Listing Dominus Litis Evaluations</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Court</th>
      <th>Evaluation</th>
      <th>Comment</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dominus_litis_evaluations.each do |dominus_litis_evaluation| %>
      <tr>
        <td><%= dominus_litis_evaluation.user_id %></td>
        <td><%= dominus_litis_evaluation.court_id %></td>
        <td><%= dominus_litis_evaluation.evaluation %></td>
        <td><%= dominus_litis_evaluation.comment %></td>
        <td><%= link_to 'Show', dominus_litis_evaluation %></td>
        <td><%= link_to 'Edit', edit_dominus_litis_evaluation_path(dominus_litis_evaluation) %></td>
        <td><%= link_to 'Destroy', dominus_litis_evaluation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

