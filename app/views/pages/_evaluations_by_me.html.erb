
<% @courts.each do |court| %>
						
	<% if court.loco_evaluation && court.loco_evaluation.evaluator == current_user %>
					

		<button class="btn btn-default btn-xs" type="button" data-toggle="collapse" data-target="#collapseMeLocoEval_<%= court.loco_evaluation.id %>" aria-expanded="false" aria-controls="collapseExample">
			<% if court.loco_evaluation.evaluation == "happy" %>
	           <i class="fa fa-smile-o fa-lg fa-success"></i>
	      <% elsif court.loco_evaluation.evaluation == "ok" %>
	            <i class="fa fa-meh-o fa-lg fa-warning"></i>
	      <% else %>
	            <i class="fa fa-frown-o fa-lg fa-danger"></i>
	      <% end %>
			
		</button>

              
		
				
	<% end %>
<% end %>


