<% @courts.each do |court| %>
						
	<% if court.loco_evaluation && court.loco_evaluation.evaluator == current_user %>
					
		<div class="collapse" id="collapseMeLocoEval_<%= court.loco_evaluation.id %>">
			
			<ul class="list-group">
		        <li class="list-group-item">
			        
			        "<i><%= court.loco_evaluation.comment %></i>"
		             <div class="text-right"><small>(<%= t("courts.evaluation_about") %> <%= link_to court.loco_evaluatee.last_name_or_placeholder, court_path(court) %>)</small></div>
			    </li>
			</ul>
		</div>
				
	<% end %>
<% end %>

